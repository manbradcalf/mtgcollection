<link rel="stylesheet" href="css/styles.css" />
<div class="cardlist-wrapper">
  {{#each this}}
  <div class="cardlist-item">
    <img class="cardlist-item-img" src={{image_uris.normal}} />
    <div class="cardlist-item-name">
      <p>{{name}}
      <p>
    </div>
    <p class="price-text">${{prices.usd}}</p>
  </div>
  {{/each}}
</div>
